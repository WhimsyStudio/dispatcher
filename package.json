{
  "name": "@ws/dispatcher",
  "version": "0.0.1.alpha-1",
  "description": "Tiny web wroker toolkit",
  "scripts": {
    "dev": "webpack serve --config webpack.config.dev.js",
    "build": "rimraf ./build && webpack --config webpack.config.prod.js && tsc --project tsconfig.build.json && npm run copy",
    "copy": "copyfiles -f ./src/lib/package.json ./build/@ws/dispatcher && copyfiles -f ./LICENSE ./build/@ws/dispatcher",
    "test:e2e-open": "cypress open",
    "test:e2e-headless": "cypress run",
    "test:native": "bun test",
    "doc": "typedoc --options ./typedoc.json "
  },
  "author": "Okay6 (whimsystudio@163.com)",
  "homepage": "www.whimsystudio.ct.ws/dispatcher",
  "keywords": [
    "web worker"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/whimsystudio/dispatcher"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-typescript": "^7.26.0",
    "@types/bun": "^1.2.9",
    "@types/jquery": "^3.5.32",
    "@types/lodash": "^4.17.16",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "babel-loader": "^9.2.1",
    "bun": "^1.2.9",
    "copyfiles": "^2.4.1",
    "cypress": "^14.2.1",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "html-webpack-plugin": "^5.6.3",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "^3.5.0",
    "rimraf": "^6.0.1",
    "terser-webpack-plugin": "^5.3.11",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typedoc": "^0.28.7",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "worker-loader": "^3.0.8"
  }
}
